<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hannah.gradecalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MathTheme">
        <activity
            android:name=".Home"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Courses"
            android:label="@string/title_activity_courses"
            android:theme="@style/MathTheme.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MATH135"
            android:label="MATH 135"
            android:parentActivityName="com.example.hannah.gradecalculator.Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.hannah.gradecalculator.Home"/>
        </activity>
        <activity
            android:name=".MATH137"
            android:label="MATH 137"
            android:parentActivityName="com.example.hannah.gradecalculator.Home"
            android:screenOrientation="portrait"/>
        <activity android:name=".CS135"
            android:label="CS 135"
            android:theme="@style/CSTheme"
            android:parentActivityName="com.example.hannah.gradecalculator.Home"
            android:screenOrientation="portrait"/>
    </application>

</manifest>